{
  "name": "ts-oauth2-server-xt",
  "version": "1.0.0",
  "description": "OAuth2 Server Typescript ",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run eslint",
    "serve": "node dist/app.js",
    "watch": "nodemon dist/app.js",
    "test": "jest --coverage --passWithNoTests",
    "reformat": "npm run prettier && npm run eslint",
    "prettier": "prettier --write '**/*.ts'",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "eslint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon $NODE_DEBUG_OPTION dist/app.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "license": "MIT",
  "_moduleAliases": {
    "@": "dist/",
    "@middleware": "dist/middleware/",
    "@models": "dist/models/",
    "@routes": "dist/routes/",
    "@services": "dist/services/",
    "@util": "dist/util/",
    "@views": "dist/views/"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.5.3",
    "bcrypt": "^4.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "config": "^3.3.0",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.11.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-oauth-server": "^2.0.0",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.0",
    "html-to-text": "^5.1.1",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "node-ssh": "^7.0.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.5",
    "sequelize-typescript": "^1.1.0",
    "winston": "^3.2.1",
    "winston-sentry-raven-transport": "^1.2.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.2",
    "@types/express-validator": "^3.0.0",
    "@types/lodash": "^4.14.149",
    "@types/multer": "^1.4.2",
    "@types/node": "^13.7.7",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3"
  }
}
