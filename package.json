    "build:clean": "shx rm -rf ./dist",
    "build": "npm-run-all build:clean build:test:*",
    "build:test:src": "tsc -p tsconfig.json",
    "build:test:package": "node ./scripts/build-prod.js",
    "build:prod": "npm-run-all build:clean build:prod:*",
    "build:prod:src": "tsc -p tsconfig.build.json",
    "build:prod:package": "node ./scripts/build-prod.js",
    "test": "nyc mocha 'test/**/*.spec.ts' --config 'test/.mocharc.jsonc'",
    "test:clean": "shx rm -rf ./coverage && shx rm -rf ./.nyc_output"
  },
  "repository": {
